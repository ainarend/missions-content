ditionalData":{"backgroundColor":"0xFF800080","category":"cosmos","icon":"🌌"}},{"level":"5","type":"cosmos","question":"Ma olen meie kodu. Ma olen suur pall. Ma asun kosmoses. Mis ma olen?","hints":[{"text":"🏠 Sa elad minu peal."},{"text":"🌎 Ma olen sinine ja roheline."},{"text":"🪐 Olen üks paljudest planeetidest kosmoses."}],"options":[{"label":"Kuu","isCorrect":false},{"label":"Päike","isCorrect":false},{"label":"Maa","isCorrect":true}],"additionalData":{"backgroundColor":"0xFF228B22","category":"cosmos","icon":"🌌"}}]}